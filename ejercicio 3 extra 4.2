subproceso rellenador(matriz)
	definir i, j Como Entero
	
	i=0
	j=0
	
	para i=0 hasta 4
		para j=0 hasta 14
			si i=0 o i=4 entonces
				matriz(i,j)=1
			sino
				si j=0 o j=14
					matriz(i,j)=1
				SiNo
					matriz(i,j)=0
				finsi
			FinSi
		FinPara
	FinPara
	
	para i=0 hasta 4
		para j=0 hasta 14
			escribir '[' matriz(i,j) ']' sin saltar
		FinPara
		escribir ''
	FinPara
FinSubProceso

Algoritmo sin_titulo
	definir matriz Como Entero
	dimension matriz(5,15)
	
	rellenador(matriz)
	
FinAlgoritmo
