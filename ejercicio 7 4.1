funcion booleano <- comparador(arrayA, arrayB, tam)
	
	definir booleano Como Logico
	definir i, j, contador Como Entero
	
	booleano = Falso
	i=0
	j=0
	contador = 0
	
	para i=0 hasta tam-1
		para j=0 hasta tam-1
			si arrayA(i) = arrayB(j) Entonces
				contador = contador + 1
			FinSi
		FinPara
	FinPara
	
	si contador = tam Entonces
		booleano = Verdadero
	SiNo
		booleano = Falso
	FinSi
FinFuncion


