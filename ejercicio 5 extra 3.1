subproceso cambiaVocal(cad)
	
	definir cadFinal, cadena2 como caracter
	definir conta,conte,conti,conto,contu,i como entero
	
	cadFinal = ''
	cadena2 = ''
	
	conta=0
	conte=0
	conti=0
	conto=0
	contu=0
	
	i=0
	
	para i=0 hasta longitud(cad)-1
		cadena2 = subcadena(cad,i,i)
		Segun cadena2 Hacer
			'a' o 'A':
				conta = conta+1
				si conta>1 entonces
					cadFinal = concatenar(cadFinal,'')
				SiNo
					cadFinal = concatenar(cadFinal, cadena2)
				FinSi
			'e' o 'E':
				conte = conte+1
				si conte>1 entonces
					cadFinal = concatenar(cadFinal,'')
				SiNo
					cadFinal = concatenar(cadFinal, cadena2)
				FinSi
			'i' o 'I':
				conti = conti+1
				si conti>1 entonces
					cadFinal = concatenar(cadFinal,'')
				SiNo
					cadFinal = concatenar(cadFinal, cadena2)
				FinSi
			'o' o 'O':
				conto = conto+1
				si conto>1 entonces
					cadFinal = concatenar(cadFinal,'')
				SiNo
					cadFinal = concatenar(cadFinal, cadena2)
				FinSi
			'u' o 'U':
				contu = contu+1
				si conto>1 entonces
					cadFinal = concatenar(cadFinal,'')
				SiNo
					cadFinal = concatenar(cadFinal, cadena2)
				FinSi
			De Otro Modo:
				cadFinal = Concatenar(cadFinal,cadena2)
		Fin Segun
		
	FinPara
	
	escribir cadFinal
	
FinSubProceso

Algoritmo sin_titulo
	
	definir frase como caracter
	
	escribir 'Ingrese una frase'
	leer frase
	
	cambiaVocal(frase)
	
	
FinAlgoritmo
