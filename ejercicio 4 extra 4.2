subproceso multiplicador(array1, array2)
	definir array, i, j como entero
	dimension array(3,3)
	i=0
	j=0
	
	para i=0 hasta 2
		para j=0 hasta 2
			array(i,j) = array1(i,j) * array2(i,j)
		FinPara
	FinPara
	
	escribir '----------------------------------------------------'
	escribir 'Matriz multiplicada:'
	escribir '----------------------------------------------------'
	para i=0 hasta 2
		para j=0 hasta 2
			escribir '[' array(i,j) ']' sin saltar
		FinPara
		escribir ''
	FinPara
	escribir '----------------------------------------------------'
	
FinSubProceso

